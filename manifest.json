{
  "name": "Hater list",
  "description": "Keep track of all the things you hate",
  "version": "1.0",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "popup.html"
  },
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["censorWords.js"]
    },
    {
      "matches": ["https://www.youtube.com/watch?v=*"],
      "js": ["censorWords.js", "yt-mute.js"]
    }
  ],
  "permissions":[
    "storage",
    "notifications",
    "contextMenus"
  ]
}

