{
  "manifest_version": 3,

  "name": "Hater list",
  "description": "Keep track of all the things you hate",
  "version": "1.0",
  "incognito": "split",

  "chrome_url_overrides": {
    "newtab": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "notifications",
    "contextMenus"
  ]
}