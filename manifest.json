{
  "name": "Hater list",
  "description": "Keep track of all the things you hate and do all things you love",
  "version": "1.0",
  "incognito": "split",
  "options_page": "options.html",
  "chrome_url_overrides": {
    "newtab": "popup.html"
  },
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["assets/jquery-3.3.1.min.js","censorWords.js"]
    }
  ],
  "permissions":[
    "storage",
    "notifications",
    "contextMenus",
    "tabs"
  ]
}

